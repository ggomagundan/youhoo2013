<% provide(:title,'Report Volunteer') %>
<script src="http://code.jquery.com/jquery-latest.js"></script>
<script>
jQuery(document).ready(function($){
  $("a[rel='colorbox']").colorbox({
    transition:'elastic',.
    opacity:'0.7',.
    maxHeight:'90%'
  });
 
  $("a.gallery").colorbox({
    rel:'group'
  });
  $("a[rel='colorboxvideo']").colorbox({
    iframe:true,.
    transition:'elastic',.
    opacity:'0.7',
    innerWidth:'60%',.
    innerHeight:'80%'
  });
});
</script>
<%= render 'layout' %>
